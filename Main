				TextLabel.TextWrapped = true
				TextLabel.TextXAlignment = Enum.TextXAlignment.Left

				Box.Name = "Box"
				Box.Parent = DrawToggle
				Box.BackgroundColor3 = Color3.fromRGB(57, 58, 81)
				Box.BorderSizePixel = 0
				Box.Position = UDim2.new(0.904999971, 0, 0.137999997, 0)
				Box.Size = UDim2.new(0, 20, 0, 20)

				UICorner_2.CornerRadius = UDim.new(0, 5)
				UICorner_2.Parent = Box

				UIStroke.Color = Color3.fromRGB(38, 39, 61)
				UIStroke.Thickness = 1.500
				UIStroke.Parent = Box

				ImageLabel.Parent = Box
				ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				ImageLabel.BackgroundTransparency = 1.000
				ImageLabel.BorderSizePixel = 0
				ImageLabel.Position = UDim2.new(0.100000001, 0, 0.100000061, 0)
				ImageLabel.Size = UDim2.new(0, 15, 0, 15)
				ImageLabel.Image = "rbxassetid://12543135185"
				ImageLabel.ImageColor3 = Color3.fromRGB(169, 158, 213)
				ImageLabel.ImageTransparency = 1.000

				local db = false
				
				uis.InputBegan:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.Keyboard then
						if key ~= nil then
							if input.KeyCode == Enum.KeyCode[key] then
								if not db then
									ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 0}):Play()
								else
									ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 1}):Play()
								end
								db = not db
								callback(db)
								
								library.flags[tostring(name)] = db
							end
						end
					end
				end)

				DrawToggle.MouseButton1Click:Connect(function()
					if not db then
						ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 0}):Play()
					else
						ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 1}):Play()
					end
					
					db = not db
					callback(db)
					
					library.flags[tostring(name)] = db
				end)
				
				spawn(function()
					while getgenv().loadingCfg do
						if library.flags[tostring(name)] then
							ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 0}):Play()
						else
							ts:Create(ImageLabel,TweenInfo.new(.5),{ImageTransparency = 1}):Play()
						end

						callback(library.flags[tostring(name)])
					end
				end)

				local KeybindFrame = Instance.new("Frame")
				local UICorner = Instance.new("UICorner")
				local TextBox = Instance.new("TextBox")

				KeybindFrame.Name = "KeybindFrame"
				KeybindFrame.Parent = DrawToggle
				KeybindFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 30)
