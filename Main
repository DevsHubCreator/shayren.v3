				end)
			end
			
			function drawSections:CreateColorPicker(name,starter,callback)
				resize(29 + 4.7)
				callback = callback or function() end

				starter = starter or {255,255,255}
				
				library.flags[tostring(name)] = false

				local DrawColorpicker = Instance.new("TextButton")
				local TextLabel = Instance.new("TextLabel")
				local UICorner = Instance.new("UICorner")
				local ActualColor = Instance.new("Frame")
				local UICorner_2 = Instance.new("UICorner")
				local UIStroke = Instance.new("UIStroke")
				local ColorChanger = Instance.new("Frame")
				local UICorner_3 = Instance.new("UICorner")
				local UIStroke_2 = Instance.new("UIStroke")
				local Wheel = Instance.new("ImageButton")
				local Ring = Instance.new("ImageLabel")
				local Slider = Instance.new("ImageButton")
				local Slide = Instance.new("Frame")
				local UICorner_4 = Instance.new("UICorner")
				local UIStroke_3 = Instance.new("UIStroke")
				local UIStroke_4 = Instance.new("UIStroke")
				local UICorner_5 = Instance.new("UICorner")
				local ImageLabel = Instance.new("ImageLabel")

				DrawColorpicker.Name = "DrawColorpicker"
				DrawColorpicker.Parent = Frame
				DrawColorpicker.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
				DrawColorpicker.BorderSizePixel = 0
				DrawColorpicker.Position = UDim2.new(0.000234730542, 0, 0.020844385, 0)
				DrawColorpicker.Size = UDim2.new(0, 271, 0, 29)
				DrawColorpicker.AutoButtonColor = false
				DrawColorpicker.Font = Enum.Font.SourceSans
				DrawColorpicker.Text = ""
				DrawColorpicker.TextColor3 = Color3.fromRGB(0, 0, 0)
				DrawColorpicker.TextSize = 14.000

				TextLabel.Parent = DrawColorpicker
				TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.BackgroundTransparency = 1.000
				TextLabel.BorderSizePixel = 0
				TextLabel.Position = UDim2.new(0.0304054171, 0, 0.241379306, 0)
				TextLabel.Size = UDim2.new(0, 208, 0, 14)
				TextLabel.Font = Enum.Font.Gotham
				TextLabel.Text = name
				TextLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
				TextLabel.TextScaled = true
				TextLabel.TextSize = 14.000
				TextLabel.TextWrapped = true
				TextLabel.TextXAlignment = Enum.TextXAlignment.Left

				UICorner.CornerRadius = UDim.new(0, 5)
				UICorner.Parent = DrawColorpicker

				ActualColor.Name = "ActualColor"
				ActualColor.Parent = DrawColorpicker
				ActualColor.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				ActualColor.BorderSizePixel = 0
				ActualColor.Position = UDim2.new(0.837638378, 0, 0.237930819, 0)
				ActualColor.Size = UDim2.new(0, 37, 0, 15)

				UICorner_2.CornerRadius = UDim.new(0, 5)
				UICorner_2.Parent = ActualColor

				UIStroke.Color = Color3.fromRGB(214, 214, 214)
				UIStroke.Thickness = 1.500
				UIStroke.Transparency = 0.500
				UIStroke.Parent = ActualColor

				ColorChanger.Name = "ColorChanger"
				ColorChanger.Parent = DrawColorpicker
				ColorChanger.BackgroundColor3 = Color3.fromRGB(31, 31, 52)
				ColorChanger.BorderSizePixel = 0
				ColorChanger.Position = UDim2.new(0.00369003601, 0, 1.10344827, 0)
				ColorChanger.Size = UDim2.new(0, 136, 0, 119)
				ColorChanger.Visible = false

				UICorner_3.Parent = ColorChanger

				UIStroke_2.Color = Color3.fromRGB(45, 45, 76)
				UIStroke_2.Thickness = 1.500
				UIStroke_2.Parent = ColorChanger

				Wheel.Name = "Wheel"
				Wheel.Parent = ColorChanger
				Wheel.Active = false
				Wheel.BackgroundColor3 = Color3.fromRGB(248, 248, 248)
				Wheel.BackgroundTransparency = 1.000
				Wheel.Position = UDim2.new(0, 0, 0.0300000608, 0)
				Wheel.Selectable = false
				Wheel.Size = UDim2.new(0.95195806, 0, 0.96999985, 0)
				Wheel.SizeConstraint = Enum.SizeConstraint.RelativeYY
				Wheel.AutoButtonColor = false
				Wheel.Image = "rbxassetid://2849458409"

				Ring.Name = "Ring"
				Ring.Parent = Wheel
				Ring.AnchorPoint = Vector2.new(0.5, 0.5)
				Ring.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Ring.BackgroundTransparency = 1.000
				Ring.BorderSizePixel = 0
				Ring.Position = UDim2.new(0.5, 0, 0.5, 0)
				Ring.Size = UDim2.new(0.100000001, 0, 0.100000001, 0)
				Ring.SizeConstraint = Enum.SizeConstraint.RelativeYY
				Ring.Image = "rbxassetid://244221613"
				Ring.ImageColor3 = Color3.fromRGB(0, 0, 0)

				Slider.Name = "Slider"
				Slider.Parent = Wheel
				Slider.Active = false
				Slider.BackgroundColor3 = Color3.fromRGB(248, 248, 248)
				Slider.BorderSizePixel = 0
				Slider.Position = UDim2.new(1.04873955, 0, 0, 0)
				Slider.Selectable = false
				Slider.Size = UDim2.new(0.150000006, 0, 1, 0)
				Slider.SizeConstraint = Enum.SizeConstraint.RelativeYY
				Slider.AutoButtonColor = false
				Slider.Image = "rbxassetid://156579757"

				Slide.Name = "Slide"
				Slide.Parent = Slider
				Slide.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
				Slide.BackgroundTransparency = 1.000
				Slide.BorderSizePixel = 0
				Slide.Position = UDim2.new(0.019060161, 0, -0.00866326131, 0)
				Slide.Size = UDim2.new(0.980941355, 0, 0.146818325, 0)

				UICorner_4.Parent = Slide

				UIStroke_3.Color = Color3.fromRGB(53, 53, 53)
				UIStroke_3.Thickness = 2.000
				UIStroke_3.Parent = Slide

				UIStroke_4.Color = Color3.fromRGB(159, 159, 159)
				UIStroke_4.Thickness = 1.500
				UIStroke_4.Parent = Slider

				UICorner_5.Parent = Slider

				ImageLabel.Parent = Wheel
				ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				ImageLabel.BackgroundTransparency = 1.000
				ImageLabel.BorderSizePixel = 0
				ImageLabel.Position = UDim2.new(-0.388407767, 0, -0.389846712, 0)
				ImageLabel.Size = UDim2.new(1.765, 0,1.733, 0)
				ImageLabel.Image = "http://www.roblox.com/asset/?id=10822615828"
				ImageLabel.ImageTransparency = 0.500

				local db = false

				DrawColorpicker.MouseButton1Click:Connect(function()
					if not db then
						ColorChanger.Size = UDim2.new(0,0,0,0)
						ts:Create(ColorChanger,TweenInfo.new(.5),{Size = UDim2.new(0, 136, 0, 119)}):Play()
						ColorChanger.Visible = true
					else
						ts:Create(ColorChanger,TweenInfo.new(.5),{Size = UDim2.new(0, 0, 0, 0)}):Play()
						spawn(function()
							wait(.5)
							if not db then
								ColorChanger.Visible = false
							end
						end)
					end

					db = not db
				end)

				----------------------------

				local ceil,clamp,atan2,pi = math.ceil,math.clamp,math.atan2,math.pi
				local tostr,sub = tostring,string.sub
				local fromHSV = Color3.fromHSV
				local v2,udim2 = Vector2.new,UDim2.new


				local WheelDown	= false
				local SlideDown	= false

				local function toPolar(v)
					return atan2(v.y, v.x), v.magnitude;
				end

				local function radToDeg(x)
					return ((x + pi) / (2 * pi)) * 360;
				end

				local hue, saturation, value = 0, 0, 1;

				local function update()
					local c = fromHSV(hue, saturation, value);
					
					local c2 = {math.floor((c.R*255)+0.5),math.floor((c.G*255)+0.5),math.floor((c.B*255)+0.5)}

					ActualColor.BackgroundColor3 = c
					ActualColor.UIStroke.Color = c
					Slider.UIStroke.Color = c
					Wheel.ImageLabel.ImageColor3 = c

					callback(c2)
				end

				local function UpdateSlide(iX,iY)	
					local rY = iY - Slider.AbsolutePosition.y;
					local cY = clamp(rY, 0, Slider.AbsoluteSize.y - Slide.AbsoluteSize.y);
					Slide.Position = udim2(0, 0, 0, cY);

					value = 1 - (cY / (Slider.AbsoluteSize.y - Slide.AbsoluteSize.y));
					Slide.BackgroundColor3 = fromHSV(0, 0, 1-value);

					update();
				end
				local function UpdateRing(iX,iY)
					local r = Wheel.AbsoluteSize.x/2
					local d = v2(iX, iY) - Wheel.AbsolutePosition - Wheel.AbsoluteSize/2;

					if (d:Dot(d) > r*r) then
						d = d.unit * r;
					end

					Ring.Position = udim2(0.5, d.x, 0.5, d.y);

					local phi, len = toPolar(d * v2(1, -1));
					hue, saturation = radToDeg(phi)/360, len / r;
					Slider.BackgroundColor3 = fromHSV(hue, saturation, 1);

					update();
				end

				Wheel.MouseButton1Down:Connect(function()
					WheelDown = true
					UpdateRing(mouse.X,mouse.Y)
				end)
				Slider.MouseButton1Down:Connect(function()
					SlideDown = true
					UpdateSlide(mouse.X,mouse.Y)
				end)


				uis.InputEnded:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						WheelDown = false
						SlideDown = false
					end
				end)

				Wheel.MouseMoved:Connect(function()
					if WheelDown then
						UpdateRing(mouse.X,mouse.Y)
					end
				end)
				Slider.MouseMoved:Connect(function()
					if SlideDown then
						UpdateSlide(mouse.X,mouse.Y)
					end
				end)
				
				spawn(function()
					while getgenv().loadingCfg do
						callback(library.flags[tostring(name)])

						ActualColor.BackgroundColor3 = library.flags[tostring(name)]
						ActualColor.UIStroke.Color = library.flags[tostring(name)]
						Slider.UIStroke.Color = library.flags[tostring(name)]
						Wheel.ImageLabel.ImageColor3 = library.flags[tostring(name)]
					end
				end)
			end

			return drawSections;
		end
		
		return drawGroup;
	end
	
	function drawTab:CreateConfig()

		local cfgTab = drawTab:CreateTab("Config")
		local cfgSection = cfgTab:CreateLeftGroup()
		cfgSection:CreateButton("Save",function(x)
			Save()
		end)
		cfgSection:CreateButton("Load",function(x)
			for i,v in next,game:GetService('HttpService'):JSONEncode(readfile(Name)) do
				if library.flags[i] then
					library.flags[i] = v 
				end
			end
			
			spawn(function()
				getgenv().loadingCfg = true
				wait(.1)
				getgenv().loadingCfg = false
			end)
		end)
	end
	
	return drawTab;
end

return library;

